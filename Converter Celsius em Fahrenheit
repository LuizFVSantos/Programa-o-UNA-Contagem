import java.util.Scanner;
import java.io.*;

class Main {
  public static void main(String[] args) {
     double C, F ;    
    
		Scanner ler = new Scanner(System.in);
    System.out.print("Digite a temperatura em C°: ");
    C = ler.nextDouble();
    F = ((C*1.8)+32);
    System.out.println("Em Fahrenheit (F°) é: " + F + "°" );
	}
}
